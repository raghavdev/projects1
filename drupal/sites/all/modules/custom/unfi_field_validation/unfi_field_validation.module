<?php

/**
 * @file
 * A collection of custom validatation for UNFI
 */

/**
 * Implementation of hook_ctools_plugin_directory().
 */
function unfi_field_validation_ctools_plugin_directory($module, $plugin) {
  if ($module == 'field_validation' && $plugin == 'validator') {
    return 'plugins/' . $plugin;
  }
}